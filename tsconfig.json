{
    "compilerOptions": {
      "baseUrl": "./",
      "paths": {
        "@": [
          ""
        ],
        "@components/*": [
          "src/components/*"
        ],
        "@contexts/*": [
          "contexts/*"
        ],
        "@assets/*": [
          "assets/*"
        ]
      }
    },
    "extends": "expo/tsconfig.base",
    "include": [
      "**/*.ts",
      "**/*.tsx",
      ".expo/types/**/*.ts",
      "expo-env.d.ts"
    ]
  }